{
    "version": 4,
    "include": [
        "../../../../CMake/arm-gcc.json",
        "../../../../config/user-tools-repos.json",
        "../../../../config/user-prefs.json"
    ],
    "configurePresets": [
        {
            "name": "Si5575_CEVB_Debug",
            "inherits": [
                "arm-gcc-cortex-preset",
                "user-tools-repos",
                "user-prefs"
            ],
            "hidden": false,
            "cacheVariables": {
                "TARGET_BOARD": "Si5575_CEVB",
                "TARGET_NAME": "Si5575_CEVB_Debug",
                "RTOS": "AzureRTOS",
                "TARGET_SERIES": "EFM32GG11",
                "SUPPORT_ANY_BASE_CONVERSION": "OFF",
                "NF_FEATURE_RTC": "ON",
                "NF_FEATURE_DEBUGGER": "ON",                
                "NF_FEATURE_HAS_SDCARD": "OFF",
                "NF_FEATURE_HAS_CONFIG_BLOCK": "ON",
                "NF_FEATURE_WATCHDOG": "OFF",
                "NF_PROFILE_NEW_ALLOCATIONS": "ON",
                "NF_TRACE_MEMORY_STATS": "ON",
                "API_System.Math": "ON",
                "API_Hardware.GiantGecko": "ON",
                "API_System.Device.Gpio": "ON",
                "API_System.Device.Pwm": "ON",
                "API_System.IO.Ports": "OFF",
                "API_System.Device.Adc": "OFF",
                "API_System.Device.Dac": "OFF",
                "API_System.Net": "OFF",
                "API_nanoFramework.Device.OneWire": "ON",
                "API_nanoFramework.Devices.Can": "OFF",
                "API_nanoFramework.ResourceManager": "ON",
                "API_nanoFramework.System.Collections": "ON",
                "API_nanoFramework.System.Text": "ON",
                "API_nanoFramework.GiantGecko.Adc": "OFF",
                "API_Windows.Storage": "OFF",
                "API_nanoFramework.Graphics": "OFF",
                "TARGET_SERIAL_BAUDRATE": "921600",
                "HAL_WP_USE_SERIAL": "OFF",
                "HAL_WP_USE_USB_CDC": "ON",
                "API_System.Device.Spi": "OFF",
                "API_Com.SkyworksInc.NanoFramework.Devices.Spi": "ON",
                "API_System.Device.I2c": "OFF",
                "API_Com.SkyworksInc.NanoFramework.Devices.I2c": "ON",
                "API_System.Device.UsbStream": "ON",
                "API_nanoFramework.System.IO.Hashing": "ON"
            }
        },
        {
            "name": "Si5575_CEVB_Release",
            "inherits": [
                "Si5575_CEVB_Debug"
            ],
            "hidden": false,
            "cacheVariables": {
                "TARGET_NAME": "Si5575_CEVB_Release",
                "NF_BUILD_RTM": "ON",
                "NF_PROFILE_NEW_ALLOCATIONS": "OFF",
                "NF_TRACE_MEMORY_STATS": "OFF"
            }
        }
    ],
    "buildPresets": [
        {
            "inherits": "base-user",
            "name": "Si5575_CEVB_Debug",
            "displayName": "Si5575_CEVB_Debug",
            "configurePreset": "Si5575_CEVB_Debug"
        },
        {
            "inherits": "base-user",
            "name": "Si5575_CEVB_Release",
            "displayName": "Si5575_CEVB_Release",
            "configurePreset": "Si5575_CEVB_Release"
        }
    ]
}
